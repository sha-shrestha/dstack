    "gh-pages": "^3.1.0",
    "lodash-es": "^4.17.15",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react": "file:../website/node_modules/react",
    "react-dom": "file:../website/node_modules/react-dom",
    "react-i18next": "file:../website/node_modules/react-i18next",
    "react-router": "file:../website/node_modules/react-router",
    "react-router-dom": "file:../website/node_modules/react-router-dom",
